                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
	 CONVERT(bigint, D.[SalesNumber]) AS SalesNumber
	,CONVERT(int, D.[SalesSeq]) AS SalesSeq
	,CAST(D.SalesDetailTypeCode as nvarchar(2)) AS [SalesDetailTypeCode]
	,D.SalesTypeCode
	,D.CustomerNumber
	,D.ChainNumber
	,CONVERT(varchar(20), CONCAT(D.ChainNumber, '-', D.CustomerNumber)) AS CustomerCode
	,D.[SalesDeliveryDate]
	,H.[GLDate]
	,D.[TicketNumber]
	,D.[ItemNumber]
	,D.[Units]	
	,D.[UnitPrice]
	,D.[SalesAmount]
	,D.[DiscountAmount]
	,D.[UnitDiscount]
	,H.OrderNumber
	,CAST(H.OrderType as nvarchar(2)) AS OrderType
	,H.LoadDate
	,CONVERT(int, H.DriverNumber) AS DriverNumber
	,CONVERT(varchar(10), H.RouteNumber) AS RouteNumber
	,CONVERT(int, H.SubRouteNumber) AS SubRouteNumber
	,H.DistCenterNumber 
	,ISC.ItemCost AS UnitCost
	,CONVERT(nvarchar(10), ltrim(rtrim(D.CreditReasonCode))) AS CreditReasonCode
	,D.GrossLB 
	,'ERMS' AS SourceSystem
	,R.RebateAmount
	,D.SLDCHGDTE
	,D.SLDCRTDTE
	,LTRIM(RTRIM(CAST(D.PriceGroup AS NVARCHAR(10)))) as PriceGroupKey
FROM ERMS.SalesDetail D
INNER JOIN ERMS.SalesHeader H ON D.SalesNumber = H.SalesNumber
LEFT JOIN 
	(SELECT *, row_number() OVER(PARTITION BY x.ItemNumber,x.[MCTPLTNUM], x.EffDate_BOM_int ORDER BY x.[MCTEFFDTE] desc) AS rownum
	 FROM [stage].[ItemStandardCost] x) ISC 
	ON ISC.ItemNumber = D.ItemNumber
	AND D.SalesDeliveryDate BETWEEN ISC.EffDate_BOM_int AND ISC.EffDate_EOM_int
	AND H.DistCenterNumber = ISC.[MCTPLTNUM] -- *** Have to add DC in order to avoid dupes
	AND rownum = 1
LEFT JOIN [ERMS].[SalesRebate] R ON R.SalesNumber = D.SalesNumber
		AND R.ItemNumber = D.ItemNumber
		AND R.ChainNumber = D.ChainNumber
		AND R.CustomerNumber = D.CustomerNumber
WHERE H.[GLDate] &gt;= 20150101
AND H.UpdateStatus = 1
AND SalesDetailTypeCode IN (1,2,3)</property>
